function main do
    a : [int];
    b : [bool];

    a = alloc int[10];

    a[0] = 20;

    a[1] = 10;
    
    a[3] = 2;

    b = alloc bool[3];

    b[0] = true;

    b[1] = false;

    b[2] = true;

    printInt(a[0]);
    printInt(a[1]);
    printInt(a[2]);
    printInt(a[3]);

    printBool(b[0]);
    printBool(b[1]);
    printBool(b[2]);

    free a;
    free b;

    return;
end
