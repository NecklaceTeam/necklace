function main do
  a: bool;
  b: bool;
  a = true;
  b = false;

  if (a && b) do
    printInt(0);
  else
    printInt(1);
  end

  if (a && true) do
    printInt(0);
  else
    printInt(1);
  end

  if (b && false) do
    printInt(0);
  else
    printInt(1);
  end

  return;
end
